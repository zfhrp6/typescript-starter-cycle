{
  "name": "typescript-starter-cycle",
  "version": "2.0.0",
  "description": "An opinionated starter for Cycle.js projects powered by TypeScript",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack serve --content-base ./dist --hot --inline --color --port 3030 --open --history-api-fallback",
    "start": "npm run dev",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npx eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier:lint": "prettier --check src",
    "prettier:fix": "prettier --write src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyclejs-community/typescript-starter-cycle.git"
  },
  "keywords": [
    "typescript",
    "cyclejs",
    "cycle",
    "starter",
    "webpack"
  ],
  "author": "Sudarsan Balaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyclejs-community/typescript-starter-cycle/issues"
  },
  "homepage": "https://github.com/cyclejs-community/typescript-starter-cycle",
  "dependencies": {
    "@cycle/dom": "^22.8.0",
    "@cycle/history": "^8.0.0",
    "@cycle/http": "^15.4.0",
    "@cycle/isolate": "^5.2.0",
    "@cycle/run": "^5.5.0",
    "csstips": "^1.2.0",
    "csx": "^10.0.2",
    "ramda": "^0.27.1",
    "switch-path": "^1.2.0",
    "typestyle": "^2.1.0",
    "xstream": "^11.14.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^2.3.2",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "yargs": "^17.0.1"
  }
}
